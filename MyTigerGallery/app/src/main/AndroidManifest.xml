<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vebitot.mytigergallery" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_label" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Explore_activity"
            android:label="@string/explore" >
        </activity>
        <activity
            android:name=".details_activity"
            android:label="@string/description" >
        </activity>
        <activity
            android:name=".NewEntry"
            android:label="@string/title_activity_new_entry" >
        </activity>
    </application>

</manifest>
